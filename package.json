{
  "homepage": "http:mun911.github.io/sap1",
  "name": "Sapper-TailwindUI-Starter",
  "description": "A template for using sapper (typescript) with Tailwind UI",
  "version": "0.0.1",
  "scripts": {
    
    "build:tailwind": "tailwindcss build -o src/tailwind.css",
    "predev": "npm run build:tailwind",
    "dev": "sapper dev",
    "prebuild": "cross-env NODE_ENV=production npm run build:tailwind",
    "deploy": "gh-pages -d build",
    "build": "cross-env NODE_ENV=production sapper build --legacy",
    "export": "cross-env NODE_ENV=production sapper export --legacy",
    "start": "node __sapper__/build",
    "validate": "svelte-check --ignore src/node_modules/@sapper"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "cross-env": "^7.0.3",
    "polka": "next",
    "sirv": "^1.0.0",
    "svelte-typed-js": "^1.0.3",
    "tailwindcss": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@rollup/plugin-url": "^5.0.0",
    "@tailwindcss/aspect-ratio": "^0.2.0",
    "@tailwindcss/forms": "^0.2.1",
    "@tailwindcss/typography": "^0.3.1",
    "@tsconfig/svelte": "^1.0.10",
    "@types/compression": "^1.7.0",
    "@types/node": "^14.11.1",
    "@types/polka": "^0.5.1",
    "autoprefixer": "^10.0.4",
    "gh-pages": "^3.1.0",
    "postcss": "^8.1.14",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sapper": "^0.28.0",
    "svelte": "^3.17.3",
    "svelte-check": "^1.0.46",
    "svelte-preprocess": "^4.3.0",
    "tslib": "^2.0.1",
    "typescript": "^4.0.3"
  }
}
